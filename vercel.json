{
    "rewrites": [
      {
        "source": "/assets/(.*)",
        "destination": "/assets/$1"
      },
      {
        "source": "/(.*).js",
        "destination": "/$1.js"
      },
      {
        "source": "/(.*).css",
        "destination": "/$1.css"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  